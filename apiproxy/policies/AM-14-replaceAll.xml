<AssignMessage name='AM-14-replaceAll'>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignVariable>
    <Name>assigned</Name>
    <Template>{replaceAll(messageid,'[0-9]','X')}</Template>
  </AssignVariable>
  <AssignVariable>
    <Name>casename</Name>
    <Value>replaceAll(messageid, '[0-9]', 'X')</Value>
  </AssignVariable>
</AssignMessage>

<AssignMessage name='AM-7'>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignVariable>
    <Name>alpha</Name>
    <Value>ABCDEFGHIJKLMNOPQRSTUVWXYZ</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>plus_4</Name>
    <Value>4</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>zero</Name>
    <Value>0</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>assigned</Name>
    <Template>{substring(alpha,zero,plus_4)}</Template>
  </AssignVariable>
  <AssignVariable>
    <Name>casename</Name>
    <Template>substring('{alpha}', {zero}, {plus_4})</Template>
  </AssignVariable>
</AssignMessage>

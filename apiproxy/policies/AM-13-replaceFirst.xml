<AssignMessage name='AM-13-replaceFirst'>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignVariable>
    <Name>assigned</Name>
    <Template>{replaceFirst(messageid,'[0-9]','X')}</Template>
  </AssignVariable>
  <AssignVariable>
    <Name>casename</Name>
    <Value>replaceFirst(messageid, '[0-9]', 'X')</Value>
  </AssignVariable>
</AssignMessage>

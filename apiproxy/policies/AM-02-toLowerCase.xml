<AssignMessage name='AM-02-toLowerCase'>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignVariable>
    <Name>text</Name>
    <Template>The quick Brown Fox Jumped Over the Lazy Dog.</Template>
  </AssignVariable>
  <AssignVariable>
    <Name>assigned</Name>
    <Template>{toLowerCase(text)}</Template>
  </AssignVariable>
  <AssignVariable>
    <Name>casename</Name>
    <Template>toLowerCase({text})</Template>
  </AssignVariable>
</AssignMessage>

<AssignMessage name='AM-6'>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignVariable>
    <Name>alpha</Name>
    <Value>ABCDEFGHIJKLMNOPQRSTUVWXYZ</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>minus_4</Name>
    <Value>-4</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>assigned</Name>
    <Template>{substring(alpha,minus_4)}</Template>
  </AssignVariable>
  <AssignVariable>
    <Name>casename</Name>
    <Template>substring('{alpha}',{minus_4})</Template>
  </AssignVariable>
</AssignMessage>

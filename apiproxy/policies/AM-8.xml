<AssignMessage name='AM-8'>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignVariable>
    <Name>alpha</Name>
    <Value>abcdefghijklmnopqrstuvwxyz</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>minus_4</Name>
    <Value>-4</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>zero</Name>
    <Value>0</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>assigned</Name>
    <Template>{substring(alpha,zero,minus_4)}</Template>
  </AssignVariable>
  <AssignVariable>
    <Name>casename</Name>
    <Template>substring('{alpha}', {zero}, {minus_4})</Template>
  </AssignVariable>
</AssignMessage>
